{
  "version": "2.0",
  "customHandler": {
    "description": {
        "defaultExecutablePath": "Rscript",
        "arguments": [
        "handler.R"
      ]
    },
    "enableForwardingHttpRequest": true
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[1.*, 2.0.0)"
  }
}
